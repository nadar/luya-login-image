{
	"name" : "nadar/luya-login-image",
	"description" : "LUYA admin login background image service.",
	"type" : "luya-extension",
	"keywords" : ["luya"],
	"license": "MIT",
	"authors" : [
		{
			"name": "Basil Suter",
			"email": "basil@nadar.io"
		}
	],
	"require": {
		"luyadev/luya-module-admin" : "^2.0|^3.0"
	},
	"require-dev" : {
	    "luyadev/luya-testsuite" : "^1.0"
	},
	"autoload" : {
		"psr-4" : {
			"Nadar\\LoginImage\\" : "src/"
		}
	},
	"autoload-dev" : {
       "psr-4" : {
           "VENDOR\\NAME\\tests\\" : "tests/"
       }
	},
	"extra" : {
		"luya" : {
			"bootstrap": [
				"Nadar\\LoginImage\\LoginImageBootstrap"
			]
		}
	}
}
